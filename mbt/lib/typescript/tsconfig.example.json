{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./examples",
    "rootDir": "./examples",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "allowJs": true
  },
  "include": [
    "examples/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src",
    "proto-gen"
  ]
}
